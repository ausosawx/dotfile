@article{YanHongWei;2017,
  title = {{加荷速度对混凝土强度的影响}},
  author = {闫红伟;},
  year = {2017},
  journal = {科学技术创新},
  number = {22},
  pages = {137--138},
  issn = {1673-1328},
  abstract = {混凝土强度等级是混凝土结构设计时强度计算取值的依据,同时也是混凝土施工中控制工程质量和工程验收时的重要依据。混凝土是典型的复合材料,影响混凝土强度的因素众多,加荷速度是重要的因素之一,但国家规范给定的加荷速度是一个比较大的范围。本文通过研究加荷速度对强度的影响,得出加荷速度增加使混凝土的强度测定值增大的幅度,为更加准确地评定混凝土的强度等级提供了参考。},
  langid = {chinese},
  lccn = {23-1600/N},
  keywords = {加荷速度,抗压强度,混凝土,研究}
}
